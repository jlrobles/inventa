<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Crud\PersonaBundle\Entity\Video" table="video">
    <id name="idVideo" type="integer" column="id_video">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="30" nullable="false"/>
    <field name="descripcion" type="string" column="descripcion" length="140" nullable="false"/>
    <field name="puntos" type="integer" column="puntos" nullable="true"/>
    <field name="numValoraciones" type="integer" column="num_valoraciones" nullable="true"/>
    <field name="url" type="string" column="url" length="50" nullable="false"/>
  </entity>
</doctrine-mapping>
